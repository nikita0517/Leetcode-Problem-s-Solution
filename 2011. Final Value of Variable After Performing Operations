class Solution {
public:
    int finalValueAfterOperations(vector<string>& op) {
        int x=0;
        for(int i=0;i<op.size();i++){
            string c=op[i];
            if(c=="--X"||c=="X--")
                    x=x-1;
            else 
                    x=x+1;
            }
        return x;
    }
};
